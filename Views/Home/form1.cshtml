@model IEnumerable<AfCentral_MVC.Models.Facilitator>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Contact</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var item in Model)
    { 
        <tr>
            <td>@item.id</td>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Email</td>
            <td>@item.Contact</td>
        </tr>  
    }
    </tbody>
</table>





